{
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "ConnectionStrings": {
        "Default": ""
    },
    "Courses": {
        "PerPage": 10,
        "Order": {
            "By": "Rating",
            "Ascending": false,
            "Allow": ["Title", "Rating", "CurrentPrice"]
        }
    },
    "CachedLife": {
        "Duration": 60
    },
    "MemoryCache": {
        "SizeLimit": 1000
    },
    "DistributedCache": {
        "Redis": {
            "Configuration": "localhost",
            "InstanceName": "MyCourseRedisCache"
        },
        "SqlServer": {
            "ConnectionString": "Data Source=(localdb)\\MSSQLLocalDb;Integrated Security=True",
            "SchemaName": "dbo",
            "TableName": "MyCourseSqlCache"
        }
    }
}